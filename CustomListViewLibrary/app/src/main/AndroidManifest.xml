<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lib.listview.custom.com.customlistviewlibrary" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="lib.listview.custom.com.customlistviewlibrary.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="lib.listview.custom.com.customlistviewlibrary.CustomListAdaptor"
            android:label="@string/title_activity_custom_list_adaptor" >
        </activity>
        <activity
            android:name="lib.listview.custom.com.customlistviewlibrary.CustomListAdapter"
            android:label="@string/title_activity_custom_list_adapter" >
        </activity>
    </application>

</manifest>
